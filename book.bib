	file = {Full Text PDF:/Users/carriewright/Zotero/storage/R9QW4VVG/Bonham and Stefan - 2017 - Women are underrepresented in computational biolog.pdf:application/pdf;Snapshot:/Users/carriewright/Zotero/storage/G2MBNSPK/article.html:text/html}
}

@article{chaudhary_ten_2020,
